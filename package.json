{
  "name": "@lehcode/soakp-client",
  "version": "1.0.1",
  "description": "A browser library for the OpenAI API for secure handling of your API key.",
  "repository": {
    "type": "git",
    "url": "git@github.com:lehcode/soakp-client.git"
  },
  "keywords": [
    "browser",
    "client",
    "module",
    "openai",
    "soakp"
  ],
  "author": "Lehcode",
  "license": "MIT",
  "main": "./demo/js/soakp-client.js",
